
#include<bits/stdc++.h>

vector<int> searchRange(vector<int>& a, int x) {

    int n = a.size();

    int first = lower_bound(a.begin(), a.end(), x) - a.begin();

    int last = upper_bound(a.begin(), a.end(), x) - a.begin() - 1;

    vector<int> ans;

    if(first==n || a[first]!=x)

    {

        first=-1; last =-1;

    }

    ans.push_back(first);

    ans.push_back(last);

    return ans;

}
